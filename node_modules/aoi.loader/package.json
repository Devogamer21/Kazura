{
  "name": "aoi.loader",
  "version": "1.0.3",
  "description": "an unofficial command loader for aoi.js gen 6",
  "main": "./lib/cjs/main.js",
  "module": "./lib/esm/main.js",
  "exports": {
    "import": "./lib/esm/main.js",
    "require": "./lib/cjs/main.js"
  },
  "types": "typings.d.ts",
  "keywords": [
    "handler",
    "command loader",
    "aoi.js",
    "aoijs"
  ],
  "author": "Cyberghxst",
  "license": "ISC",
  "dependencies": {
    "aoi.js": "^6.4.0",
    "ascii-table": "^0.0.9",
    "tty-table": "^4.2.1"
  },
  "devDependencies": {
    "@akarui/aoi.db": "^1.0.0",
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "eslint": "^8.45.0",
    "lodash": "^4.17.21",
    "uglify-js": "^3.17.4"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "scripts": {
    "compile": "babel src --out-dir lib/cjs --config-file ./babel.cjs.json && babel src --out-dir lib/esm --config-file ./babel.esm.json && node packers.js",
    "test": "node index.js"
  }
}